{
  "name": "@jarvis-agent/web",
  "version": "0.1.2",
  "description": "Browser/Web environment support for Jarvis Agent framework.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "exports": {
